@INPROCEEDINGS{bown09,
  author = {Bown, Oliver},
  title = {Against Individual Creativity},
  booktitle = {Computational Creativity: An Interdisciplinary Approach},
  year = {2009},
  number = {09291},
  series = {Dagstuhl Seminar Proceedings},
  address = {Dagstuhl, Germany},
  abstract = {In this paper I discuss reasons for viewing human creativity more
	as a social process than as an individual act. 
	
	These reasons include the subjectivity of evaluation in attributing
	creativity, the potentially arbitrary relationship between individuals
	and creativity at the cultural level, the importance of the capacity
	for preserving cultural information over and above the capacity to
	innovate, the role of objects, institutions and interaction in sparking
	creativity, and the social constructedness of creative domains. 
	
	I consider the consequences of this way of thinking for research into
	computational creativity. 
	
	I argue that realising the goals of computational creativity depends
	on integrating research on creative agents with social technologies.},
  annote = {Argues that we should pay more attention to cultural influences and
	the role of the collective, in modelling creativity, and that an
	agent-based approach is needed to simuate a creative community appropriately.
	
	Bown criticises computational creativity approaches for focussing
	on creativity of an individual (`individualist cognitive science'\cite[p.
	1]{bown09}) in preference to creativity in a socio-cultural context
	(`distributed, social approach'\cite[p. 2]{bown09}).
	
	
	Csikszentmihalyi (1996) - individual creativity is only recognised
	via the people involved in that activity (the field and the domain)
	
	`Csikszentmihalyi argues that it is impossible to conceive of an objective
	measurement of creativity based only on the action of an individual
	with respect to an existing domain. We have no objective grounds
	upon which to determine how creative something is, and must rely
	on individual responses to the behaviour in question, such as collective
	or expert evaluations. Therefore, an evaluation of creativity must
	be the result of a group's response to the individual's activity.
	Correspondingly, Csikszentmihalyi defines creativity as the process
	of changing a domain.'\cite[p. 3]{bown09}
	
	Is a Picasso, who works locked away in a room and never displays his
	output, less creative than Picasso as we know him? More accurately,
	is locked-away Picasso _perceived_ as less creative than his unhidden
	counterpart? [NB How does the locked-away Picasso receive influences
	from external sources, inspiration from the external world, constructive
	feedback during his creative workings, etc?]},
  bdsk-url-1 = {http://drops.dagstuhl.de/opus/volltexte/2009/2205},
  date-added = {2010-10-07 13:56:58 +0100},
  date-modified = {2011-07-26 18:48:43 +0100},
  read = {1}
}

