@BOOK{maykut94,
  title = {Beginning qualitative research: A philosophic and practical guide},
  publisher = {Routledge},
  year = {1994},
  author = {Maykut, P.S. and Morehouse, R.},
  address = {London, UK},
  annote = {Constant comparison method - inspired by grounded theory but more
	descriptive and not aimed at deriving a grounded theory, but aimed
	at summarising/abstracting from data. Step by step, didactic description
	of method (from pre-computational days! I've updated it for computational
	copy-pasting rather than physical copy-pasting...) 1. Split up data
	into self contained units of meaning. - Tag each unit with index
	to show where it came from. 2. Paste together recurring concepts/phrases/topics/patterns/themes.
	- Label each recurrring concept (/etc) with the prominent idea. This
	label acts as a provisional category name. 3. Go through all data,
	unit by unit, to see if it fits each provisional category. - NB Categories
	can include positive or negative examples 4. For each provisional
	category, write a rule (hypothesis?) that captures the meaning of
	this data = provisional statement of category. Include the key properties
	of this category. 5. Refine preliminary codes to category names,
	gradually. 6. Check categories for overlap/ambiguity. 7. Check any
	'miscellaneous (uncategorised) data and categorise if important.
	In reality this process would take a long time esp step 3, so some
	shortcuts etc would prob happen.},
  date-added = {2010-08-30 11:49:25 +0100},
