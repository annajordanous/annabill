@INPROCEEDINGS{pease01,
  author = {Pease, Alison and Winterstein, Daniel and Colton, Simon},
  title = {Evaluating Machine Creativity},
  booktitle = {Proceedings of Workshop Program of ICCBR-Creative Systems: Approaches
	to Creativity in AI and Cognitive Science},
  year = {2001},
  pages = {129-137},
  abstract = {We consider aspects pertinent to evaluating creativity to be input,
	output and the process by which the output is achieved. These issues
	may be further divided, and we highlight associated justifications
	and controversies. Appropriate methods of measuring these aspects
	are suggested and discussed.},
  annote = {Paper considers input, output and process for creativity - specifically
	how to measure elements associated with each of these aspects. Considers
	generation and evaluation of creativity. Wide-ranging discussion
	of the issues involved with some introductory suggestions for standardised
	assessment methods. These assessment methods are evaluated w.r.t.
	1) "to what extent do they reflect human evaluations of creativity",
	and 2) "how applicable are they" (p137). 1) can be evaluated empirically
	and 2) can be evaluated by trying to apply the methods to AI programs.
	PWC suggest a possible need to refine creativity as a concept ("<i>creativity2</i>"(p137))so
	that it is more defined and therefore the criteria by which it should
	be measured become clearer - cf Popper's falsifiable hypotheses.
	[on following on from their suggested starting points for evaluation]
	"research on evaluating machine creativity can proceed scientifically,
	by a series of falsifiable claims. We hope that this will lead to
	a deeper nderstanding of the nature of creativity."(PWC 01 p137)
	--- Introduction 129 Assumptions Input 130 * Input Measure The Inspiring
	Set Output 130-134 Novelty Novelty relative to a body of knowledge
	* Transformational Measure Novelty relative to complexity * Complexity
	Measure Novelty relative to an archetype * Archetypal Measure Novelty
	as surprise * Surprise Measure Perceived novelty * Perceived Novelty
	Measure Quality Quality relative to emotional response * Emotional
	Response Measure Quality relative to an aim * Pragmatic Measure Process
	134-137 Methods of generation 135 Randomness * Randomness Measure
	Methods of evaluation 135-137 Evaluating the item * Evaluation of
	Item Measure Evaluating the process * Evaluation of Process Measure
	Evaluation of machine creativity How to measure success How to attribute
	creativity Analogy to the attribution of intelligence Conclusion
	and further work 137 --- Assumptions - (at least some part of) creativity
	is domain independent (therefore general measurements are preferable)
	"[if this assumption is incorrect] then the attempt will nevertheless
	reveal much of the nature of creativit: which aspects are domain
	dependent and why, thus guiding research." (p129) - use human creativity
	as a guide for computational theory and as a yardstick to judge progress
	against. - no distinction between genius and everyday creativity:
	"We assume that creativity in children, adults and geniuses is essentially
	the same phenomenon." (p129) - creativity = generation + evaluation
	- the creative process results in some item(s) being produced (but
	"items do not have to be material objects"(p129)) - define some necessary
	criteria to capture what is "<i>actually<> creative" as opposed to
	"<i>potentially</> creative" (p129) - selection for actual creativity
	rather than a more encompassing 'creative until proved otherwise'
	approach. [interesting approach - didn't realise the distinction.
	What if we were to define negative criteria, i.e. not creative if
	criteria X is satisfied? or a combination?] - 2-fold aims of computational
	creativity research: 1. understand creativity (in general) more 2.
	make useful practically-productive programs "Boden suggests that
	it is helpful to regard aspects such as novelty, quality and process
	as dimensions of creativity. Instead of asking 'is <x> creative?'
	(assuming a boolean judgement) or 'how creative is <x>?' (assuming
	a linear judgement) we should ask 'where does <x> lie in a creativity
	space?' (assuming an n-dimensional space for n criteria where we
	can measure each dimension." (p136) creativity space-how to define
	this practically? (p137) "we must structure our research by developing
	theoretical (which are the key aspects and why?) and practical (how
	can we measure them in our programs?) approaches to machine creativity.
	It is currently more appropriate to ask 'is P1 or P2 more creative
	in aspect A?' (particularly if P1 and P2 are versions of the same
	program) rather than 'where does <x> lie in creativity space?'."
	},
  date-added = {2008-04-17 15:10:01 +0100},
  date-modified = {2010-07-23 18:33:21 +0100},
  keywords = {machine creativity, measures of creativity, evaluating novelty, evaluating
	quality, concept spaces},
  read = {1}
}

@ARTICLE{peinado10MM,
  author = {Peinado, F. and Francisco, V. and Herv{\'a}s, R. and Gerv{\'a}s,
	P.},
  title = {Assessing the Novelty of Computer-Generated Narratives Using Empirical
	Metrics},
  journal = {Minds and Machines},
  year = {2010},
  volume = {20},
  pages = {565-588},
  number = {4},
  abstract = {Novelty is a key concept to understand creativity. Evaluating a piece
	of artwork or other creation in terms of novelty requires comparisons
	to other works and considerations about the elements that have been
	reused in the creative process. Human beings perform this analysis
	intuitively, but in order to simulate it using computers, the objects
	to be compared and the similarity metrics to be used should be formalized
	and explicitly implemented. In this paper we present a study on relevant
	elements for the assessment of novelty in computer-generated narratives.
