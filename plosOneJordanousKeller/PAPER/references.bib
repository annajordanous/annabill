@INPROCEEDINGS{shibata02,
  author = {Shibata, Hirohito and Hori, Koichi},
  title = {A System to Support Long-term Creative Thinking in Daily Life and
	its Evaluation},
  booktitle = {Creativity and Cognition Conference},
  year = {2002},
  pages = {142-149},
  address = {Loughborough, Leicestershire, UK},
  abstract = {Most current creativity support systems support short-term temporal
	thinking that is separate from users' daily activities. In this paper,
	we propose a system that supports long-term idea- generation in daily
	life. The system consists of two subsystems: a management system
	for problems and ideas called IdeaManager; and a personal information
	storage system called iBox. When information is registered in iBox,
	it searches related problems and ideas in IdeaManager and then presents
	the results. Then users try to generate or enhance ideas for automatically
	retrieved problems or ideas using registered information as the hint.
	Our target users are those who must generate novel ideas and acquire
	relevant information for a certain problem or theme, such as researchers
	or planners. To evaluate and enhance our system, we carried out a
	six-week experiment. During this period, subjects managed their problems
	and ideas in their daily activities. In the experiment, several methods
	of searching for related problems and ideas in IdeaManager were compared.
	Based on the results, we give some proposals for future systems.
	},
  annote = {Look at their evaluation technique},
  bdsk-file-1 = {},
  date-added = {2009-03-11 14:42:23 +0000},
  date-modified = {2009-06-17 15:46:57 +0100},
  keywords = {Creativity support system, Computer-aided thinking, Information management
	system}
}

@ARTICLE{shoham93,
  author = {Shoham, Yoav},
  title = {Agent-Oriented Programming},
  journal = {Artificial Intelligence},
  year = {1993},
  volume = {60},
  pages = {51-92},
  number = {1},
  annote = {AOP = specialisation of OOP
	
	state of agent = [beliefs, decisions, capabilities, obligations, ***]
	= mental state <described in formal logics>
	
	*** this also includes knowledge. Obligations may have temporal qualities
	associated with them.
	
	Definition of agent as part of AOP: `An agent is an entity whose state
	is viewed as consisting of mental components such as beliefs, capabilities,
	choices and commitments. ... In this view, therefore, agenthood is
	in the mind of a programmer: What makes any hardware or software
	component an agent is precisely the fact that one has chosen to analyze
	and control it in these mental terms.'\cite[p. 52]{wooldridge02}
	
	 In other words, anything that can be ascribed as having mental states
	is potentially an agent, c.f. Dennett's intentional stance - anything
	can be viewed this way - but what gains from being viewed like this?
	
	3 Primary Components of an AOP system (i) Formal specification system
	for mental states (ii) A (universal) communications language (iii)
	``agentifier'' (making agents out of things)
	
	Shoham (1993) covers (i) and (ii), not (iii), and focusses on (ii)
	
	MENTAL STATES barebones: { decisions / choices, beliefs, capabilities,
	obligations } NB decisions = obligations to oneself
	
	Different apps - may need to add to/augment this set. 
	
	Key issues: time, actions (and effects of actions)
	
	MODAL STATEMENTS
	
	[fi] is an action or a fact being TRUE NB [fi] can be recursive
	
	Belief: B^t_a [fi] - agent a believes at time t that [fi] Obligation:
	OBL^t_[a,b] [fi] - agent a is committed to agent b at time t about
	[fi] and Decisions: DEC^t_a [fi] = OBL^t_[a,a] [fi] - agent a is
	committed to itself at time t about[fi] NB Agent a has to believ
	that [fi] is true or achievable Capability*: CAN^t_a [fi] - agent
	a _can_ do [fi] at time t (doesn't necessarily mean it will do it)
	* NB is capability a _mental_ state?ABLE_a [fi] = CAN^time([fi])_a
	[fi] time([fi]) = furthest away time mentioned in [fi]
	
	ASSUMPTIONS - internal consistency of beliefs and obligations [interesting
	- what if there is a clash? e.g. if beliefs are not logically consistent?]
	- An agent will only commit to what it is capable of - An agent is
	aware of its own obligations - persistence of mental states (unless
	proved false) - context-sensitive (appropriate in certain contexts
	- may change in others e.g. be obligated to print a document, as
	long as the network is functioning)
	
	NB Cohen and Levesque's terminology is slightly different: (BEL a
	p) + (G a p) + temporal operations e.g. diamond for sometime (where
	BEL is belief and G is Goal)
	
	Shoham only| shared across both | Cohen and Levesque only ----------------------------|----------------------------------|--------------------------------------------
	- obligation| - belief | - goals - capability| - time| - intentions
	
	
	
	FLOW OF CONTROL IN AGENT 0 see diagram on p56 for flow chart version
	of this:
	
	initialise -> [update beliefs -> update commitments/obligations] <->
	EXECUTE (---> internal actions \& messages out)
	
	Agent's Beliefs, commitments/obligations and abilities/capacities
	also feed into EXECUTE, as well as into the individual 'update' actions
	for each mental state },
  date-added = {2010-09-28 16:33:25 +0100},
  date-modified = {2010-09-28 17:03:19 +0100},
  read = {1}
}

