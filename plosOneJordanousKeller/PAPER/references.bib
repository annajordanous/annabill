@BOOK{strauss98,
  title = {Basics of Qualitative Research: Techniques and Procedures for Developing
	Grounded Theory},
  publisher = {Sage},
  year = {1998},
  author = {Strauss, Anselm and Corbin, Juliet},
  address = {London, UK},
  edition = {2nd},
  annote = {See Corbin and Strauss 1990 for methodological notes. Chapter on
	evaluation (and also notes here from Corbin and Strauss 1990) Scientific
	canons (standards) to evaluate quantitative work by: - Significance
	- Theory-observance compatability - Generalisability (predictive
	power) - Consistency - Reproducibility - Precision - Verification
	Evaluating a study: 1. Data validity, relability, credibility 2.
	Judgements about theory produced: it's plausibility and value 3.
	Fairness, rigour and adequacy of work. 4. Empirical grounding of
	work Strauss and Corbin concentrate on points 3 and 4 (saying that
	points 1 and 2 are covered by the above canons) and generate 7 criteria
	for evaluation of a derived grounded theory. These criteria are very
	specific to grounded theory, and occasionally unorthodox due to this
	specificity (e.g. criteria 4), checking that each stage of analysis
	has been done. However each can be viewed more generally, outside
	of a strict grounded theory perspective. 1. How was the original
	data sample selected? - Fairly uncontentious question to ask in evaluation
	- applicable more generally 2. What major categories emerged? - cf.
	What are the key contributions of this work? 3. What data supports
	the major categories? - cf. How are the key contributions of the
	work supported by the research? 4. How did theory guide data collection?
	- cf. accepting that an investigator influences what data they collect
	and putting a positive spin on this 5. What hypotheses were formulated
	and how were they tested? - cf. What principles can be drawn from
	the data (again not a contentious evaluative reflection) 6. How were
	discrepancies for hypotheses dealt with? - cf. What were intermediate
	hypotheses and how were they tested [NB perhaps this is dealt with
	sufficiently in addressing criteria 5] 7. How and why were the core
	findings identified? - cf. How rigorous was the research approach?},
  date-modified = {2010-08-30 13:21:27 +0100},
  read = {1}
}

