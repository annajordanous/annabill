@BOOK{russell95,
  title = {Artificial Intelligence: a Modern Approach},
  publisher = {Prentice Hall},
  year = {1995},
  author = {Russell, Stuart Jonathan and Norvig, Peter},
  address = {Englewood Cliffs, NJ},
  annote = {Chapter 2: Intelligent Agents
	
	An agent PERCEIVES environment through SENSORS and ACTS on environment
	through EFFECTORS
	
	Rational agent = `one that does the right thing'\cite[p. 31]{russell95}
