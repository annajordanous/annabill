@ARTICLE{mandel11,
  author = {Mandel, Gregory N.},
  title = {To Promote the Creative Process: Intellectual Property Law and the
	Psychology of Creativity},
  journal = {Notre Dame Law Review (online)},
  year = {2011},
  volume = {86},
  number = {1},
  abstract = {Though a primary goal of intellectual property law is to promote creativity
	in technology and the arts, intellectual property doctrine pays remarkably
	little attention to psychology research on how to advance creativity.
	Psychology studies on motivation, collaboration, and divergent versus
	convergent cognitive thought processes provide significant insight
	into the creative process and indicate that certain intellectual
	property law hinders the very creativity the law is designed to inspire.
	These insights intersect at an issue vital to the forefront of creative
	achievement: promoting large-scale collaborative creativity. Large-scale
	collaborative projects have become critical in many areasof innovaiton
	due to the need for multidiscipinary expertise and substantial resources
	to push the envelope of human knowledge. From partnerships across
	private, government, and universtity research sectors to open and
	collaborative peer production, large-scale collaboration is revolutionizing
	fields as diverse as software, film, music, and biotechnology. The
	psychology of creativity provides valuable lessons on how to advance
	these efforts.},
  annote = {\begin{quote} `Despite the value of facilitating creativity for intellectual
	property law, understanding creativity is hardly something within
	the competent domain of law and legal analysis. Not surprisingly,
	the legislative and judicial development of intellectual property
	law has paid remarkably little attention to what is known about how
	to promote creativity.' \cite[p. *** CHECK ***]{mandel11} \end{quote}
	
	Looking at intellectual property (IP) law in the context of psychological
	research into creativity,\citeauthor{mandel11} argues that intellectual
	property law treats copyrightable work as primarily the work of an
	individual, needing adaptation to adequately accomodate collaborational
	work. 
	
	\citeauthor{mandel11} also questions the role of intellectual property
	law as a motivational factor for creativity. Describing intellectual
	property as `the primary area through which the law seeks to motivate
	and regulate human creativity' \cite[p. *** CHECK ****]{mandel11},
	\citeauthor{mandel11} notes that IP law is an extrinsic rather than
	intrinsic motivational factor, in other words it provides an external
	reward for the results of the creative process rather than being
	an internal motivational process arising from within the creative
	individual. This results-driven motivational reward is less effective
	as a motivator for creative productive work than if an individual
	is inherently motivated to be creative \cite{amabile96}, unless it
	can be internalised in some way. 
	
	One example of such internalisation is in the {\em Rock the House}
	Intellectual Property project \cite{rockthehouse11} devised by Mike
	Weatherley MP. This project promotes awareness of music copyright
	laws via a band competition that is built around musicians' internal
	motivations to promote and publicise their music. Hence there is
	some intrinsic motivation for band musicians to assist towards the
	end goal of the project: to encourage musicians to copyright their
	creative work. 
	
	
	},
  date-added = {2011-03-17 16:56:14 +0000},
  date-modified = {2012-03-21 19:39:57 +0000},
  keywords = {intellectual property, creativity, psychology},
  read = {1}
}

