@ARTICLE{gallie56,
  author = {Gallie, W. B.},
  title = {Essentially Contested Concepts},
  journal = {Proceedings of the Aristotelian Society},
  year = {1956},
  volume = {56},
  pages = {167-198},
  annote = {Creativity can be seen as an example of what Gallie described as
	\textit{essentially contested concepts} \cite{gallie56}: concepts
	which are subjective and whose meaning is commonly understood but
	elusive to define, with a variety of meanings attached to that concept.
	
	
	Essentially contested concepts usually cannot be measured directly
	and objectively, but can be used for qualitative judgement of worth;
	however their subjective and abstract nature makes it difficult for
	people to pin down these judgements in words in a way which is exact,
	complete and non-disputable. Gallie gives examples of (works of)
	art, democracy and social justice as essentially contested concepts.
	Though he does not mention creativity, it is not a great stretch
	of imagination to see how creativity could also be seen as an essentially
	contested concept. 
	
	The value of identifying creativity as an essentially contested concept
	comes in Gallie's discussions of how these abstract notions can be
	better understood through reflecting on this type of concept more
	generally, helping us override difficulties caused by disputes about
	a concept's exact meaning. 
	
	Gallie defines essentially contested concepts through several features,
	such as being internally complex in nature, but amenable to being
	broken down into identifiable constituent elements of varying relative
	importance, dependent on a number of factors such as context and
	individual preference. That this variety of interpretations exists
	is contributory to the coining of the descriptor of these concepts
	as 'essentially contested'; though there is consensus on the concepts'
	meaning in very general terms, exact interpretations differ. There
	is not a single agreed instantiation of these concepts but instead
	many reasonable possibilities. The influences of changing circumstances
	and contexts mean that essentially contested concepts, by their nature,
	will never be resolved to a single meaning or instantiation and their
	interpretation will continue to be contestable. },
  isbn = {00667374},
  publisher = {Blackwell Publishing on behalf of The Aristotelian Society},
  url = {http://www.jstor.org/stable/4544562}
}

