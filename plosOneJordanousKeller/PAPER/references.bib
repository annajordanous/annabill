@ARTICLE{jennings10MM,
  author = {Jennings, K. E.},
  title = {Developing Creativity: Artificial Barriers in Artificial Intelligence},
  journal = {Minds and Machines},
  year = {2010},
  volume = {20},
  pages = {489-501},
  number = {4},
  abstract = {The greatest rhetorical challenge to developers of creative artificial
	intelligence systems is convincingly arguing that their software
	is more than just an extension of their own creativity. This paper
	suggests that ``creative autonomy,'' which exists when a system not
	only evaluates creations on its own, but also changes its standards
	without explicit direction, is a necessary condition for making this
	argument. Rather than requiring that the system be hermetically sealed
	to avoid perceptions of human influence, developing creative autonomy
	is argued to be more plausible if the system is intimately embedded
	in a broader society of other creators and critics. Ideas are presented
	for constructing systems that might be able to achieve creative autonomy.},
  date-added = {2010-11-20 13:22:36 +0000},
  date-modified = {2011-03-24 11:45:21 +0000},
  keywords = {Computational creativity, Autonomy, Socially-inspired computing},
  read = {0}
}

@INPROCEEDINGS{jennings08,
  author = {Jennings, Kyle E.},
  title = {Developing Creativity: Artificial Barriers in Artificial Intelligence},
  booktitle = {Proceedings of the 5th International Joint Workshop on Computational
	Creativity},
  year = {2008},
  pages = {1-10},
  address = {Madrid, Spain},
  abstract = {The greatest rhetorical challenge to developers of creative artificial
	intelligence systems is convincingly arguing that their software
	is more than just an extension of their own creativity. This paper
	suggests that the key feature this requires is "creative autonomy,"
	which exists when a system not only evaluates creations on its own,
	but also changes its standards without explicit direction. Paradoxically,
	developing creative autonomy is argued to require that the system
	be intimately embedded in a broader society of other creators and
	critics. A sketch is provided of a system that might be able to achieve
	creative autonomy, provided it initially liked others in the society
	to different extents, and had to remain "proud" of its past work.
	This should lead to emergent dynamics that enable the creative AI
	to be more than a simple blend of the influences it was predicated
	upon, though this awaits empirical demonstration.},
  annote = {'creative autonomy' - if a system can self-evaluate its work then
	change its standards [evaluative criteria? operations?] The Quest
	for Creative Autonomy Criteria Creative Autonomy and Creativity Learning
	to Evaluate Subjectivity Making Evaluations Communication Changing
	Standards Additional Behaviors Affinity Pride Bootstrapping and Changing
	$\varepsilon_{ii}$ Cognitive Dissonance False Inferences About Preferences
	Selective Acceptance Seeking Autonomy Revisited Conclusions Criteria
	for "creative autonomy":(p2) - autonomous evaluation - autonomous
	change (of standards) - non-randomness Contradiction in that a creative
	system that can behave autonomously actually needs to become more
	embedded in a society of creative activity and criticism, for that
	autonomy to be useful in a wider context. ``Paradoxically, developing
	creative autonomy is argued to require that the system be intimately
	embedded in a broader society of other creators and critics.'' \cite[p.
	1]{jennings08} ``human creativity, which is clearly autonomous, takes
	place within a rich web of social interactions.'' \cite[p. 2]{jennings08}
	Q. Is human creativity ``clearly autonomous''? Perhaps a less troublesome
	way of expressing this is that we want computational creators to
	operate with a similar degree of autonomy to human creators. Autonomy
	is a contributing factor to human creativity, but needs to be accompanied
	by proactiveness, a purposeful intent behind actions: ``The description
	of creative autonomy offered here captures only a small part of why
	humans can be considered creative. A system whose creations had a
	style that was not easily traced to a few influences, yet was recognizably
	in same domiain, would be a major accomplishment. However, ... freedom
	from manipulation is not the same as acting purposefully'' \cite[p.
	9]{jennings08} Relevant comment on the simulations of creator/critic
	societies (regarding single critics vs multiple interacting critics):
	``In a single critic society, it is unlikely that the system's standards
	would be more than distorted agglomerations of the critic's standards.
	What's more, the ease with which the critic could manipulate the
	system would make it hard to argue that the creator was changing
	autonomously. In a multiple-critic society, complex interactions
	might make any one creator impervious to manipulation, and emergent
	dynamics of the system could lead to clusters of creative styles.
	This awaits empirical demonstration, which would still leave open
	the philosophical question of whether changing absent direct manipulation
	is the same as autonomy.'' \cite[p. 9]{jennings08} The eternal problem
	of computational creativity...: ``Creative artificial intelligence
	must always fight the impression that it is simply a fancy tool for
	expressing the programmer's creativity.''\cite[p. 9]{jennings08}
	Goes on to discuss how, rather than isolating a system from outside
	influences, it should be situated within a society of influences
	and interactions, ``though it must reach beyond the programmer''\cite[p.
	9]{jennings08}},
  date-added = {2009-04-21 10:46:27 +0100},
  date-modified = {2011-07-26 17:26:14 +0100},
  keywords = {Computational Creativity, Autonomy, Socially-inspired computing}
}

