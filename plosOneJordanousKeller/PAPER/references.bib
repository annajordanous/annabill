@ARTICLE{snyder04,
  author = {Snyder, Allan and Mitchell, John and Bossomaier, Terry and Pallier,
	Gerry},
  title = {The {Creativity Quotient}: An Objective Scoring of Ideational Fluency},
  journal = {Creativity Research Journal},
  year = {2004},
  volume = {16},
  pages = {415-420},
  number = {4},
  abstract = {A classical test for accessing the potential creativity of an individual
	is based on ideational fluency, where a person is asked to generate
	all possible uses for a familiar item like a piece of paper. In scoring
	the results, it is intuitive that the suggested uses should not be
	weighted equally. Those suggested in radically different categories
	are "worth more" than those suggested within the same category only.
	We used information theory to derive a simple mathematical expression
	for a more objective measure of ideationl fluency. We call this the
	creativity quotient (CQ). This innovative measure was examined using
	a small sample of participantst, and is illustrated by the responses
	of two typical individuals from an ideational fluency task. The CQ
	accounts for the number of ideas (fluency), plus the number of categories
	(flexibility). Ongoing research will examine the independence of
	CQ from established measures of intelligence and personality.},
  bdsk-file-1 = {},
  date-added = {2009-04-02 12:02:08 +0100},
  date-modified = {2011-03-28 23:49:26 +0100}
}

